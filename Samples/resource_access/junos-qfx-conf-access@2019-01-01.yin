<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-qfx-conf-access"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-access="http://yang.juniper.net/junos-qfx/conf/access">
  <namespace uri="http://yang.juniper.net/junos-qfx/conf/access"/>
  <prefix value="jc-access"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
  </import>
  <import module="junos-qfx-conf-root">
    <prefix value="jc"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos access configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 19.1R1.3</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="access-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="access-group"/>
  </augment>
  <grouping name="access-group">
    <container name="access">
      <description>
        <text>Network access configuration</text>
      </description>
      <uses name="juniper-access-options"/>
    </container>
  </grouping>
  <grouping name="juniper-access-options">
    <uses name="apply-advanced"/>
    <list name="radius-server">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>RADIUS server configuration</text>
      </description>
      <uses name="access-radius-server-object"/>
    </list>
    <list name="domain-name-server">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Default DNS server's IPv4 address</text>
      </description>
      <leaf name="name">
        <description>
          <text>Address</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <uses name="apply-advanced"/>
    </list>
    <list name="domain-name-server-inet">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>DNS server's IPv4 address</text>
      </description>
      <leaf name="name">
        <description>
          <text>Address</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <uses name="apply-advanced"/>
    </list>
    <list name="domain-name-server-inet6">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>DNS server's IPv6 address</text>
      </description>
      <leaf name="name">
        <description>
          <text>Address</text>
        </description>
        <type name="jt:ipv6addr"/>
      </leaf>
      <uses name="apply-advanced"/>
    </list>
    <list name="wins-server">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Default WINS server's IPv4 address</text>
      </description>
      <leaf name="name">
        <description>
          <text>Address</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <uses name="apply-advanced"/>
    </list>
    <list name="address-pool">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Address pool</text>
      </description>
      <uses name="address-pool-object"/>
    </list>
    <list name="group-profile">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Group profile to use for this client</text>
      </description>
      <uses name="group-profile-object"/>
    </list>
    <list name="profile">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Set of attributes that define access</text>
      </description>
      <leaf name="name">
        <description>
          <text>Profile name</text>
        </description>
        <type name="string">
          <length value="1 .. 63"/>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf-list name="accounting-order">
        <ordered-by value="user"/>
        <description>
          <text>Order in which accounting mechanisms are used</text>
        </description>
        <type name="enumeration">
          <enum name="radius">
            <description>
              <text>Remote Authentication Dial-In User Service</text>
            </description>
          </enum>
        </type>
      </leaf-list>
      <leaf-list name="authentication-order">
        <ordered-by value="user"/>
        <description>
          <text>Order in which authentication mechanisms are used</text>
        </description>
        <type name="enumeration">
          <enum name="radius">
            <description>
              <text>Remote Authentication Dial-In User Service</text>
            </description>
          </enum>
          <enum name="password">
            <description>
              <text>Locally configured password in access profile</text>
            </description>
          </enum>
          <enum name="none">
            <description>
              <text>No authentication performed</text>
            </description>
          </enum>
          <enum name="nasreq">
            <description>
              <text>Diameter based authentication and authorization using NASREQ application protocol</text>
            </description>
          </enum>
          <enum name="ldap">
            <description>
              <text>Lightweight Directory Access Protocol</text>
            </description>
          </enum>
          <enum name="securid">
            <description>
              <text>RSA SecurID Authentication</text>
            </description>
          </enum>
        </type>
      </leaf-list>
      <leaf-list name="authorization-order">
        <ordered-by value="user"/>
        <description>
          <text>Order in which authorization mechanisms are used</text>
        </description>
        <type name="enumeration">
          <enum name="jsrc">
            <description>
              <text>Diameter based authorization using Juniper's SRC application protocol</text>
            </description>
          </enum>
          <enum name="nasreq">
            <description>
              <text>Diameter based authorization using NASREQ application protocol</text>
            </description>
          </enum>
          <enum name="none">
            <description>
              <text>No authorization performed</text>
            </description>
          </enum>
        </type>
      </leaf-list>
      <leaf name="provisioning-order">
        <description>
          <text>Order in which provisioning mechanisms are used</text>
        </description>
        <type name="enumeration">
          <enum name="jsrc">
            <description>
              <text>Address authorization via SRC</text>
            </description>
          </enum>
          <enum name="gx-plus">
            <description>
              <text>Policy/service provisioning via GXPLUS</text>
            </description>
          </enum>
          <enum name="pcrf">
            <description>
              <text>Policy/service provisioning via PCRF</text>
            </description>
          </enum>
        </type>
      </leaf>
      <leaf-list name="preauthentication-order">
        <ordered-by value="user"/>
        <description>
          <text>Order in which preauthentication mechanisms are used</text>
        </description>
        <type name="enumeration">
          <enum name="radius">
            <description>
              <text>Remote Authentication Dial-In User Service</text>
            </description>
          </enum>
        </type>
      </leaf-list>
      <leaf-list name="charging-service-list">
        <ordered-by value="user"/>
        <description>
          <text>List of used 3gpp charging servicess</text>
        </description>
        <type name="enumeration">
          <enum name="ocs">
            <description>
              <text>Online charging service</text>
            </description>
          </enum>
        </type>
      </leaf-list>
      <list name="domain-name-server">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Default DNS server's IPv4 address</text>
        </description>
        <leaf name="name">
          <description>
            <text>Address</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf>
        <uses name="apply-advanced"/>
      </list>
      <list name="domain-name-server-inet">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>DNS server's IPv4 address</text>
        </description>
        <leaf name="name">
          <description>
            <text>Address</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf>
        <uses name="apply-advanced"/>
      </list>
      <list name="domain-name-server-inet6">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>DNS server's IPv6 address</text>
        </description>
        <leaf name="name">
          <description>
            <text>Address</text>
          </description>
          <type name="jt:ipv6addr"/>
        </leaf>
        <uses name="apply-advanced"/>
      </list>
      <list name="wins-server">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Default WINS server's IPv4 address</text>
        </description>
        <leaf name="name">
          <description>
            <text>Address</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf>
        <uses name="apply-advanced"/>
      </list>
      <list name="client">
        <key value="name"/>
        <description>
          <text>Entity requesting access</text>
        </description>
        <uses name="access-client-object"/>
      </list>
      <container name="address-assignment">
        <description>
          <text>Address assignment pool</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="pool">
          <description>
            <text>Name of address-assignment pool</text>
          </description>
          <type name="string">
            <length value="1 .. 63"/>
          </type>
        </leaf>
      </container>
      <leaf name="session-limit-per-username">
        <description>
          <text>Maximum number of sessions allowed per username</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 16"/>
          </type>
        </type>
      </leaf>
      <container name="local">
        <description>
          <text>Set configuration for local reporting - obsolete config</text>
        </description>
        <status value="deprecated"/>
        <uses name="apply-advanced"/>
        <leaf name="flat-file-profile">
          <description>
            <text>Specifies that the service accounting will be reported as per flat-file profile</text>
          </description>
          <type name="string"/>
        </leaf>
      </container>
      <container name="radius">
        <description>
          <text>Set of RADIUS configurations</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf-list name="authentication-server">
          <ordered-by value="user"/>
          <description>
            <text>The authentication server list to use in the specified order to send authentication messages</text>
          </description>
          <type name="jt:ipaddr"/>
        </leaf-list>
        <leaf-list name="accounting-server">
          <ordered-by value="user"/>
          <description>
            <text>The accounting server list to use in the specified order to send accounting messages</text>
          </description>
          <type name="jt:ipaddr"/>
        </leaf-list>
        <leaf-list name="preauthentication-server">
          <ordered-by value="user"/>
          <description>
            <text>The preauthentication server list to use in the specified order to send preauthentication messages</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf-list>
        <container name="options">
          <description>
            <text>Specifies the RADIUS options</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="ethernet-port-type-virtual">
            <description>
              <text>Type of physical port to authenticate the user is virtual (RADIUS attribute 61)</text>
            </description>
            <type name="empty"/>
          </leaf>
          <container name="interface-description-format">
            <description>
              <text>Interface description in the NAS-port-ID attribute</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="exclude-sub-interface">
              <description>
                <text>Exclude sub-interface from interface description</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="exclude-adapter">
              <description>
                <text>Exclude adapter from interface description</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="exclude-channel">
              <description>
                <text>Exclude channel from interface description</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
          <leaf name="nas-identifier">
            <description>
              <text>NAS-Identifier to be used for authentication and accounting requests (RADIUS attribute 32)</text>
            </description>
            <type name="string">
              <length value="1 .. 64"/>
            </type>
          </leaf>
          <container name="nas-port-extended-format">
            <description>
              <text>RADIUS client's use of an extended format for RADIUS attribute 5</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="ae-width">
              <description>
                <text>Number of bits for the aggregated ethernet identifier field</text>
              </description>
              <units name="bits"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="0 .. 32"/>
                </type>
              </type>
            </leaf>
            <leaf name="slot-width">
              <description>
                <text>Number of bits for the slot field</text>
              </description>
              <units name="bits"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="0 .. 32"/>
                </type>
              </type>
            </leaf>
            <leaf name="adapter-width">
              <description>
                <text>Number of bits for the adapter field</text>
              </description>
              <units name="bits"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="0 .. 32"/>
                </type>
              </type>
            </leaf>
            <leaf name="port-width">
              <description>
                <text>Number of bits for the port field</text>
              </description>
              <units name="bits"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="0 .. 32"/>
                </type>
              </type>
            </leaf>
            <leaf name="pw-width">
              <description>
                <text>Number of bits for the pseudo-wire field</text>
              </description>
              <units name="bits"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="0 .. 32"/>
                </type>
              </type>
            </leaf>
            <leaf name="stacked-vlan-width">
              <description>
                <text>Number of bits for the S-VLAN subinterface field</text>
              </description>
              <units name="bits"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="0 .. 32"/>
                </type>
              </type>
            </leaf>
            <leaf name="vlan-width">
              <description>
                <text>Number of bits for the VLAN subinterface field</text>
              </description>
              <units name="bits"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="0 .. 32"/>
                </type>
              </type>
            </leaf>
            <container name="atm">
              <description>
                <text>ATM specific parameters for NAS Port</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="slot-width">
                <description>
                  <text>Number of bits for the ATM slot field</text>
                </description>
                <units name="bits"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32">
                    <range value="0 .. 32"/>
                  </type>
                </type>
              </leaf>
              <leaf name="adapter-width">
                <description>
                  <text>Number of bits for the ATM adapter field</text>
                </description>
                <units name="bits"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32">
                    <range value="0 .. 32"/>
                  </type>
                </type>
              </leaf>
              <leaf name="port-width">
                <description>
                  <text>Number of bits for the ATM port field</text>
                </description>
                <units name="bits"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32">
                    <range value="0 .. 32"/>
                  </type>
                </type>
              </leaf>
              <leaf name="vpi-width">
                <description>
                  <text>Number of bits for the ATM VPI field</text>
                </description>
                <units name="bits"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32">
                    <range value="0 .. 32"/>
                  </type>
                </type>
              </leaf>
              <leaf name="vci-width">
                <description>
                  <text>Number of bits for the ATM VCI field</text>
                </description>
                <units name="bits"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32">
                    <range value="0 .. 32"/>
                  </type>
                </type>
              </leaf>
            </container>
          </container>
          <leaf name="nas-port-id-delimiter">
            <description>
              <text>Single character delimiter character to use in the NAS-Port-Id</text>
            </description>
            <type name="string">
              <length value="1"/>
            </type>
          </leaf>
          <container name="nas-port-id-format">
            <description>
              <text>Format methods for building the NAS-Port-Id radius attribute</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="nas-identifier">
              <description>
                <text>Include the nas-identifier value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="interface-description">
              <description>
                <text>Include the interface-description value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="agent-circuit-id">
              <description>
                <text>Include (ACI) agent-circuit-id value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="agent-remote-id">
              <description>
                <text>Include (ARI) agent-remote-id value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="postpend-vlan-tags">
              <description>
                <text>Include the vlan tag(s) using :&lt;outer&gt;-&lt;inner&gt;</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="interface-text-description">
              <description>
                <text>Include the interfaces description string</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf-list name="order">
              <ordered-by value="user"/>
              <description>
                <text>Order of options for appearance in the NAS-Port-Id string</text>
              </description>
              <type name="enumeration">
                <enum name="nas-identifier">
                  <description>
                    <text>Include the nas-identifier value</text>
                  </description>
                </enum>
                <enum name="agent-circuit-id">
                  <description>
                    <text>Include (ACI) agent-circuit-id value</text>
                  </description>
                </enum>
                <enum name="agent-remote-id">
                  <description>
                    <text>Include (ARI) agent-remote-id value</text>
                  </description>
                </enum>
                <enum name="interface-description">
                  <description>
                    <text>Include the interface-description value</text>
                  </description>
                </enum>
                <enum name="interface-text-description">
                  <description>
                    <text>Include the interface's description string</text>
                  </description>
                </enum>
                <enum name="postpend-vlan-tags">
                  <description>
                    <text>Include the vlan tag(s) using :&lt;outer&gt;-&lt;inner&gt;</text>
                  </description>
                </enum>
              </type>
            </leaf-list>
          </container>
          <container name="nas-port-type">
            <description>
              <text>Translation mechanism for changing the NAS-Port-Type radius attribute</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="ethernet">
              <description>
                <text>Translation mechanism for changing the Ethernet value</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
          <leaf name="calling-station-id-delimiter">
            <description>
              <text>Single character separator for calling-station-id</text>
            </description>
            <type name="string">
              <length value="1"/>
            </type>
          </leaf>
          <container name="calling-station-id-format">
            <description>
              <text>Format method for building the calling-station-id</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="nas-identifier">
              <description>
                <text>Include the nas-identifier value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="interface-description">
              <description>
                <text>Include the interface-description value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="agent-circuit-id">
              <description>
                <text>Include agent-circuit-id value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="agent-remote-id">
              <description>
                <text>Include agent-remote-id value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="interface-text-description">
              <description>
                <text>Include the interfaces description string</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="stacked-vlan">
              <description>
                <text>Include the stacked vlan tag value</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="vlan">
              <description>
                <text>Include the vlan tag value</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
          <leaf name="accounting-session-id-format">
            <description>
              <text>Decimal format or description format for the accounting session ID</text>
            </description>
            <type name="enumeration">
              <enum name="decimal">
                <description>
                  <text>Decimal-session-identifier</text>
                </description>
              </enum>
              <enum name="description">
                <description>
                  <text>Description Format: &lt;jnpr&gt; &lt;interface-type&gt; &lt;interface-specifier&gt;:&lt;session-identifier&gt;</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="revert-interval">
            <description>
              <text>Time after which to revert to primary server</text>
            </description>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 604800"/>
              </type>
            </type>
          </leaf>
          <leaf name="vlan-nas-port-stacked-format">
            <description>
              <text>Include the S-VLAN ID, in addition to the VLAN ID, for subscribers on Ethernet interfaces</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="client-authentication-algorithm">
            <description>
              <text>Algorithm to access the RADIUS servers for authentication</text>
            </description>
            <type name="enumeration">
              <enum name="direct">
                <description>
                  <text>Contacts the first AAA server on the list for each request, the second AAA server if the first one fails, and so on</text>
                </description>
              </enum>
              <enum name="round-robin">
                <description>
                  <text>Contacts the first AAA server for the first request, the second AAA server for the second request, and so on</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="client-accounting-algorithm">
            <description>
              <text>Algorithm to access the RADIUS servers for accounting</text>
            </description>
            <type name="enumeration">
              <enum name="direct">
                <description>
                  <text>Contacts the first AAA server on the list for each request, the second AAA server if the first one fails, and so on</text>
                </description>
              </enum>
              <enum name="round-robin">
                <description>
                  <text>Contacts the first AAA server for the first request, the second AAA server for the second request, and so on</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="juniper-dsl-attributes">
            <description>
              <text>Include the Juniper (IANA 4874) DSL VSAs in requests to RADIUS servers</text>
