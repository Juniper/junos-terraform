                  </description>
                  <type name="string">
                    <length value="1 .. 510"/>
                  </type>
                </leaf-list>
              </case>
              <case name="case_8">
                <leaf-list name="string">
                  <ordered-by value="user"/>
                  <description>
                    <text>Array of character string values</text>
                  </description>
                  <type name="string">
                    <length value="1 .. 254"/>
                  </type>
                </leaf-list>
              </case>
              <case name="case_9">
                <leaf-list name="ip-address">
                  <ordered-by value="user"/>
                  <description>
                    <text>Array of IP address values</text>
                  </description>
                  <type name="jt:ipv4addr"/>
