    </leaf>
    <leaf name="pap-password">
      <description>
        <text>PAP password</text>
      </description>
      <type name="jt:unreadable"/>
    </leaf>
    <choice name="client_type">
      <case name="case_1"/>
      <case name="case_2"/>
      <case name="case_3">
        <container name="ike">
          <presence value="enable ike"/>
          <description>
            <text>Configuration for dynamic IKE peers</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="initiate-dead-peer-detection">
            <description>
              <text>Initiate dead peer detection</text>
            </description>
            <type name="empty"/>
          </leaf>
          <container name="dead-peer-detection">
            <presence value="enable dead-peer-detection"/>
            <description>
              <text>Dead peer detection options</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="interval">
              <description>
                <text>Interval at which the DPD messages should be sent</text>
              </description>
              <default value="2"/>
              <units name="seconds"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 180"/>
                </type>
              </type>
            </leaf>
            <leaf name="threshold">
              <description>
                <text>Maximum number of DPD messages</text>
              </description>
              <default value="3"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 10"/>
                </type>
              </type>
            </leaf>
          </container>
          <container name="respond-bad-spi">
            <presence value="enable respond-bad-spi"/>
            <description>
              <text>Respond to IPsec packets with bad security parameter index (SPI) values</text>
            </description>
            <leaf name="max-responses">
              <description>
                <text>Maximum number of times to respond</text>
              </description>
              <default value="5"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 30"/>
                </type>
              </type>
            </leaf>
          </container>
          <choice name="name">
            <case name="case_1"/>
            <case name="case_2"/>
          </choice>
          <container name="reverse-route">
            <presence value="enable reverse-route"/>
            <description>
              <text>Reverse route parameters</text>
            </description>
            <uses name="apply-advanced"/>
            <container name="preference">
              <description>
                <text>Preference value</text>
              </description>
              <leaf name="metric-value">
                <description>
                  <text>Metric value</text>
                </description>
                <type name="union">
                  <type name="uint32"/>
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                </type>
              </leaf>
            </container>
          </container>
        </container>
      </case>
    </choice>
    <leaf-list name="client-group">
      <ordered-by value="user"/>
