    <list name="option">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>DHCP option</text>
      </description>
      <leaf name="name">
        <description>
          <text>DHCP option identifier code</text>
        </description>
        <type name="union">
          <type name="uint32"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf>
      <choice name="option-type-choice">
        <case name="case_1">
          <leaf name="flag">
            <description>
              <text>Boolean flag value</text>
            </description>
            <type name="enumeration">
              <enum name="true">
                <description>
                  <text>True value</text>
                </description>
              </enum>
              <enum name="false">
                <description>
                  <text>False value</text>
                </description>
              </enum>
              <enum name="on">
                <description>
                  <text>On value</text>
                </description>
              </enum>
              <enum name="off">
                <description>
                  <text>Off value</text>
                </description>
              </enum>
            </type>
          </leaf>
        </case>
        <case name="case_2">
          <leaf name="byte">
            <description>
              <text>Unsigned 8-bit value</text>
            </description>
            <type name="union">
              <type name="uint8"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
        </case>
        <case name="case_3">
          <leaf name="short">
            <description>
              <text>Signed 16-bit numeric value</text>
            </description>
            <type name="union">
              <type name="int16"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
        </case>
        <case name="case_4">
          <leaf name="unsigned-short">
            <description>
              <text>Unsigned 16-bit numeric value</text>
            </description>
            <type name="union">
              <type name="uint16"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
        </case>
        <case name="case_5">
          <leaf name="integer">
            <description>
              <text>Signed 32-bit numeric value</text>
            </description>
            <type name="union">
              <type name="int32"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
        </case>
        <case name="case_6">
          <leaf name="unsigned-integer">
            <description>
              <text>Unsigned 32-bit numeric value</text>
            </description>
            <type name="union">
              <type name="uint32"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
        </case>
        <case name="case_7">
          <leaf name="hex-string">
            <description>
              <text>Hexadecimal string</text>
            </description>
            <type name="string">
              <length value="1 .. 510"/>
            </type>
          </leaf>
        </case>
        <case name="case_8">
          <leaf name="string">
            <description>
              <text>Character string value</text>
            </description>
            <type name="string">
              <length value="1 .. 254"/>
            </type>
          </leaf>
        </case>
        <case name="case_9">
          <leaf name="ip-address">
            <description>
              <text>IP address value</text>
            </description>
            <type name="jt:ipv4addr"/>
          </leaf>
        </case>
        <case name="case_10">
          <leaf name="ipv6-address">
            <description>
              <text>IPV6 address value</text>
            </description>
            <type name="jt:ipv6addr"/>
          </leaf>
        </case>
        <case name="case_11">
          <container name="array">
            <presence value="enable array"/>
            <description>
              <text>Array of values</text>
            </description>
            <uses name="apply-advanced"/>
            <choice name="option-type-array-choice">
              <case name="case_1">
                <leaf-list name="flag">
