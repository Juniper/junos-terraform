                  </type>
                </leaf>
                <uses name="apply-advanced"/>
                <leaf name="low">
                  <description>
                    <text>Lower limit of ipv6 address range</text>
                  </description>
                  <type name="jt:ipv6prefix-mandatory"/>
                </leaf>
                <leaf name="high">
                  <description>
                    <text>Upper limit of ipv6 address range</text>
                  </description>
                  <type name="jt:ipv6prefix-mandatory"/>
