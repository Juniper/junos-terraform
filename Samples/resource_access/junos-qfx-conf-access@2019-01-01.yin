                  <description>
                    <text>Excluded address</text>
                  </description>
                  <type name="jt:ipv6addr"/>
                </leaf>
                <uses name="apply-advanced"/>
              </list>
              <list name="excluded-range">
                <key value="name"/>
                <ordered-by value="user"/>
                <description>
                  <text>Excluded address range</text>
                </description>
                <max-elements value="20"/>
                <leaf name="name">
                  <description>
                    <text>Range name</text>
                  </description>
                  <type name="string">
                    <length value="1 .. 63"/>
                  </type>
                </leaf>
                <uses name="apply-advanced"/>
                <leaf name="low">
