                  <type name="jt:ipv4addr"/>
                </leaf>
                <leaf name="high">
                  <description>
                    <text>Upper limit of excluded address range</text>
                  </description>
                  <type name="jt:ipv4addr"/>
                </leaf>
              </list>
            </container>
          </case>
          <case name="case_2">
            <container name="inet6">
              <presence value="enable inet6"/>
              <description>
                <text>IPv6</text>
              </description>
              <uses name="apply-advanced"/>
