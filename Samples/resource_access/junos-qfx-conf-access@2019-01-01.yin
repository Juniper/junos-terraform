        </type>
      </type>
    </leaf>
    <leaf name="source-address">
      <description>
        <text>Use specified address as source address</text>
      </description>
      <type name="jt:ipaddr"/>
    </leaf>
    <leaf name="routing-instance">
      <description>
        <text>Use specified routing instance</text>
      </description>
      <type name="string"/>
    </leaf>
    <leaf name="radsec-destination">
      <description>
        <text>RADSEC destination</text>
      </description>
      <type name="union">
        <type name="uint32"/>
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
      </type>
    </leaf>
  </grouping>
  <grouping name="protocol-attribute-type">
    <description>
      <text>Protocol specific attribute configuration</text>
    </description>
    <leaf name="name">
      <description>
        <text>Name for this protocol attribute group</text>
      </description>
      <type name="string">
        <length value="1 .. 63"/>
      </type>
    </leaf>
    <uses name="apply-advanced"/>
    <container name="dhcp">
      <description>
        <text>DHCPv4 configurable attributes</text>
      </description>
      <uses name="dhcp-attribute-type"/>
    </container>
    <container name="dhcpv6">
      <description>
        <text>DHCPv6 configurable attributes</text>
      </description>
      <uses name="dhcp-attribute-type"/>
    </container>
  </grouping>
  <grouping name="radsec-definition">
    <description>
      <text>RADSEC configuration definition</text>
    </description>
    <uses name="apply-advanced"/>
    <list name="destination">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>RADSEC destination configuration</text>
      </description>
      <max-elements value="16"/>
      <leaf name="name">
        <description>
          <text>RADSEC destination id</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 65535"/>
          </type>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf name="address">
        <description>
          <text>Destination IP address</text>
        </description>
        <type name="jt:ipaddr"/>
      </leaf>
      <container name="dynamic-requests">
        <presence value="enable dynamic-requests"/>
        <description>
          <text>Accept dynamic requests</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="source-address">
          <description>
            <text>Source address for dynamic requests</text>
          </description>
          <type name="jt:ipaddr"/>
        </leaf>
        <leaf name="source-port">
          <description>
            <text>Source port for dynamic requests</text>
          </description>
          <default value="2083"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="1 .. 65535"/>
            </type>
          </type>
        </leaf>
        <choice name="source-lsri-instance">
          <case name="case_1">
            <container name="logical-system">
              <description>
                <text>Logical system for dynamic requests</text>
              </description>
              <leaf name="logical-system-name">
                <description>
                  <text>Name of logical system for dynamic requests</text>
                </description>
                <type name="string"/>
              </leaf>
              <leaf name="routing-instance">
                <description>
                  <text>Routing instance for dynamic requests</text>
                </description>
                <type name="union">
                  <type name="string">
                    <pattern value="default"/>
                  </type>
                  <type name="string"/>
                </type>
              </leaf>
            </container>
          </case>
          <case name="case_2">
            <leaf name="routing-instance">
              <description>
                <text>Routing instance for dynamic requests</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="default"/>
                </type>
                <type name="string"/>
              </type>
            </leaf>
          </case>
        </choice>
      </container>
      <leaf name="id-reuse-timeout">
        <description>
          <text>Radius id may be reused after this timeout</text>
        </description>
        <default value="120"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="60 .. 3600"/>
          </type>
        </type>
      </leaf>
      <choice name="lsri-instance">
        <case name="case_1">
          <container name="logical-system">
            <description>
              <text>Logical system to be used</text>
            </description>
            <leaf name="logical-system-name">
              <description>
                <text>Name of logical system</text>
              </description>
              <type name="string"/>
            </leaf>
            <leaf name="routing-instance">
              <description>
                <text>Routing instance to be used</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="default"/>
                </type>
                <type name="string"/>
              </type>
            </leaf>
          </container>
        </case>
        <case name="case_2">
          <leaf name="routing-instance">
            <description>
              <text>Routing instance to be used</text>
            </description>
            <type name="union">
              <type name="string">
                <pattern value="default"/>
              </type>
              <type name="string"/>
            </type>
          </leaf>
        </case>
      </choice>
      <leaf name="max-tx-buffers">
        <description>
          <text>Maximum number of tx-buffers</text>
        </description>
        <default value="100"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="32 .. 3200"/>
          </type>
        </type>
      </leaf>
      <leaf name="port">
        <description>
          <text>Desitnaion port</text>
        </description>
        <default value="2083"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint16">
            <range value="1 .. 65535"/>
          </type>
        </type>
      </leaf>
      <leaf name="source-address">
        <description>
          <text>Source IP address for destination</text>
        </description>
        <type name="jt:ipaddr"/>
      </leaf>
      <leaf name="tls-certificate">
        <description>
          <text>TLS certificate</text>
        </description>
        <type name="string">
          <length value="1 .. 255"/>
        </type>
      </leaf>
      <leaf name="tls-force-ciphers">
        <type name="enumeration">
          <enum name="medium">
            <description>
              <text>Add medium grade ciphers</text>
            </description>
          </enum>
          <enum name="low">
            <description>
              <text>Add medium and low grade ciphers</text>
            </description>
          </enum>
        </type>
      </leaf>
      <leaf name="tls-min-version">
        <default value="v1.2"/>
        <type name="enumeration">
          <enum name="v1.1">
            <description>
              <text>Require tls 1.1 and 1.2</text>
            </description>
          </enum>
          <enum name="v1.2">
            <description>
              <text>Require tls 1.2</text>
            </description>
          </enum>
        </type>
      </leaf>
      <leaf name="tls-peer-name">
        <description>
          <text>Expected peer fdqn</text>
        </description>
        <type name="string">
          <length value="1 .. 255"/>
        </type>
      </leaf>
      <leaf name="tls-timeout">
        <description>
          <text>Limit on tls handshake time</text>
        </description>
        <default value="5"/>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="3 .. 90"/>
          </type>
        </type>
      </leaf>
    </list>
  </grouping>
  <grouping name="securid-server-object">
    <leaf name="name">
      <description>
        <text>Name of the SecurID server</text>
      </description>
      <type name="string"/>
    </leaf>
    <uses name="apply-advanced"/>
    <leaf name="configuration-file">
      <description>
        <text>Path to the SecurID server configuration (sdconf.rec) file</text>
      </description>
      <type name="string">
        <length value="1 .. 1024"/>
      </type>
    </leaf>
  </grouping>
</module>
