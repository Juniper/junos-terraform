                    <type name="int32"/>
                    <type name="string">
                      <pattern value="&lt;.*&gt;|$.*"/>
                    </type>
                  </type>
                </leaf-list>
              </case>
              <case name="case_6">
                <leaf-list name="unsigned-integer">
                  <ordered-by value="user"/>
                  <description>
                    <text>Array of unsigned 32-bit numeric values</text>
                  </description>
                  <type name="union">
                    <type name="uint32"/>
                    <type name="string">
                      <pattern value="&lt;.*&gt;|$.*"/>
                    </type>
                  </type>
                </leaf-list>
              </case>
              <case name="case_7">
                <leaf-list name="hex-string">
                  <ordered-by value="user"/>
                  <description>
