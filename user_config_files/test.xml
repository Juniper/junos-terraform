                <protocol>bgp</protocol>
            </from>
            <then>
                <community>
                    <add/>
                    <community-name>dc1-spine1</community-name>
                </community>
                <accept/>
            </then>
        </term>
        <term>
            <name>default</name>
            <then>
                <reject/>
            </then>
        </term>
    </policy-statement>
    <policy-statement>
        <name>IPCLOS_BGP_IMP</name>
        <term>
            <name>loopback</name>
            <from>
                <protocol>bgp</protocol>
                <protocol>direct</protocol>
            </from>
            <then>
                <accept/>
            </then>
        </term>
        <term>
            <name>default</name>
            <then>
                <reject/>
            </then>
        </term>
    </policy-statement>
    <policy-statement>
        <name>PFE-LB</name>
        <then>
